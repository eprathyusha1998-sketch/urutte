# Production Environment Variables for Urutte Frontend
# Copy this to .env.production for production deployment

# Backend API URL - Update this with your actual production backend URL
REACT_APP_API_URL=https://urutte.com/api

# WebSocket URL
REACT_APP_WS_URL=wss://urutte.com/ws

# Alternative backend URLs (uncomment the one you're using):
# REACT_APP_API_URL=https://backend.urutte.com
# REACT_APP_API_URL=https://your-backend-domain.com
# REACT_APP_API_URL=https://your-ec2-backend-ip:port

# Environment
NODE_ENV=production

# Build optimization
GENERATE_SOURCEMAP=false
